<% if(success && success.length) { %>
<div class="flash-msg alert alert-dismissible fade show" role="alert">
  <i class="ri-file-check-line flash-i"></i>
  <%= success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"></button>
</div>
<% } %> <% if(error && error.length) { %>
<div class="alert flash-msg alert-dismissible fade show" role="alert">
  <i class="ri-emotion-unhappy-line flash-i"></i>
  <i class="ri-error-warning-line"></i> <%= error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"></button>
</div>
<% } %>

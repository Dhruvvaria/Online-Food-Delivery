<% layout("layouts/boilerplate") %>

<h1>Restaurents Booking Details</h1>
<div class="restaurents">
  <% if(!allBooking.length) { %>
  <h2 class="form-container">No Booking here!!</h2>
  <% } %> <% if(allBooking.length) { %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Phone</th>
        <th scope="col">Time</th>
        <th scope="col">Adults</th>
        <th scope="col">Child</th>
        <th scope="col"></th>
      </tr>
    </thead>
    <tbody>
      <% for (booking of allBooking) { %>
      <tr>
        <td><%= booking.name %></td>
        <td><%= booking.mobile %></td>
        <td><%= booking.time %></td>
        <td><%= booking.adult %></td>
        <td><%= booking.child %></td>
        <td>
          <a href="/admin_home/restaurents_booking/<%= booking._id %>/delete"
            ><button class="btn btn-dark">Done</button></a
          >
        </td>
      </tr>
      <% } %>
    </tbody>
  </table>
  <% } %>
</div>
